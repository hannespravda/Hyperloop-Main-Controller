<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="PRG_MainController" Id="{4edc6fe0-de01-4ab3-bf7e-58f3d4baabe4}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM PRG_MainController
VAR
	fbServiceModule 			: FB_ServiceModule;
	fbLevitationSystem 			: FB_LevitationSystem;
	fbGuidanceSystem 			: FB_GuidanceSystem;
	
	eState 						: TopLevelSTM := TopLevelSTM.STARTUP;
	
	stOperationControlCommands 	: OperationControlCommands;
	stXMCCommands 				: XMCCommands;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[doDataHandling();

CASE eState OF
	////
	TopLevelSTM.STARTUP:
		doEnableSystems();
		doCheckSystem();
		
		
		IF PRG_ErrorHandling.bStartUpCheck AND NOT stOperationControlCommands.bGoToError THEN
			eState := TopLevelSTM.READY;
		ELSE
			IF PRG_ErrorHandling.bErrorInStartUp OR stOperationControlCommands.bGoToError THEN
				fbServiceModule(eState:=ServiceModuleSTM.PreparingForError);
				eState := TopLevelSTM.ERROR;
			ELSE
				eState := TopLevelSTM.STARTUP;
			END_IF
		END_IF
	////
	TopLevelSTM.READY:
		fbServiceModule.eState := ServiceModuleSTM.Waiting;
		fbLevitationSystem.eState := LevitationSystemSTM.Stationary;
		fbGuidanceSystem.eState := GuidanceSystemSTM.PreRunning;
		stXMCCommands.bStopOperational := FALSE;
		
		fbServiceModule();
		fbLevitationSystem();
		fbGuidanceSystem();


		IF PRG_ErrorHandling.bErrorInReady OR stOperationControlCommands.bGoToError THEN
			fbServiceModule(eState := ServiceModuleSTM.PreparingForError);
			eState := TopLevelSTM.ERROR;
		ELSIF stOperationControlCommands.bStartOperational THEN
			stXMCCommands.bStartOperational := TRUE;	
			eState := TopLevelSTM.OPERATIONAL;
		END_IF
	////
	TopLevelSTM.OPERATIONAL:
		fbServiceModule.eState := ServiceModuleSTM.Running;
		stXMCCommands.bStartOperational := FALSE;
		
		IF fbLevitationSystem.bTrajectoryCompleted THEN
			fbLevitationSystem.eState := LevitationSystemSTM.HoldingAirGap;
		ELSE
			fbLevitationSystem.eState := LevitationSystemSTM.Transitioning;
		END_IF
		
		IF stOperationControlCommands.bMinimumAirGapForGC THEN
			fbGuidanceSystem.eState := GuidanceSystemSTM.Running;
		ELSE
			fbGuidanceSystem.eState := GuidanceSystemSTM.PreRunning;
		END_IF
		
		fbServiceModule();
		fbLevitationSystem();
		fbGuidanceSystem();
		
		
		IF PRG_ErrorHandling.bErrorInOperational OR stOperationControlCommands.bGoToError OR stOperationControlCommands.bStopOperational THEN
			eState := TopLevelSTM.SHUTDOWN;
		END_IF
	////
	TopLevelSTM.SHUTDOWN:
		fbServiceModule.eState := ServiceModuleSTM.PostRunning;
		
		fbLevitationSystem.eState := LevitationSystemSTM.TransitioningToZero;
		
		IF stOperationControlCommands.bMinimumAirGapForGC THEN
			fbGuidanceSystem.eState := GuidanceSystemSTM.Running;
		ELSE
			fbGuidanceSystem.eState := GuidanceSystemSTM.PostRunning;
		END_IF
		
		fbServiceModule();
		fbLevitationSystem();
		fbGuidanceSystem();
		
		IF fbLevitationSystem.bTrajectoryCompleted THEN
			stOperationControlCommands.bShutdownFinished := TRUE;
			
			IF PRG_ErrorHandling.bErrorInOperational OR PRG_ErrorHandling.bErrorInShutdown OR stOperationControlCommands.bGoToError THEN
				fbServiceModule(eState := ServiceModuleSTM.PreparingForError);
				eState := TopLevelSTM.ERROR;
			ELSIF stOperationControlCommands.bEndShutdown THEN
				stXMCCommands.bStopOperational := TRUE;
				eState := TopLevelSTM.READY;
			END_IF
		END_IF
		

	////	
	TopLevelSTM.ERROR:
		IF PRG_ErrorHandling.bAllErrorsAcknowledged THEN
			stXMCCommands.bAckError := TRUE;
			eState := TopLevelSTM.STARTUP;
		END_IF
END_CASE]]></ST>
    </Implementation>
