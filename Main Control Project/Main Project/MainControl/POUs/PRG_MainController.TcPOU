    <Method Name="doEnableSystems" Id="{4402bbf9-07a3-41dd-916a-44a19b1d489b}">
      <Declaration><![CDATA[METHOD doEnableSystems
VAR_INPUT
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT stOperationControlCommands.nOperationMode.0 THEN
	fbServiceModule.eActivationStatus := ActivationStatus.Enabled;
ELSE
	fbServiceModule.eActivationStatus := ActivationStatus.Disabled;
END_IF

IF NOT stOperationControlCommands.nOperationMode.1 THEN
	fbLevitationSystem.eActivationStatus := ActivationStatus.Enabled;
ELSE
	fbLevitationSystem.eActivationStatus := ActivationStatus.Disabled;
END_IF

IF NOT stOperationControlCommands.nOperationMode.2 THEN
	fbGuidanceSystem.eActivationStatus := ActivationStatus.Enabled;
ELSE
	fbGuidanceSystem.eActivationStatus := ActivationStatus.Disabled;
END_IF





]]></ST>
      </Implementation>
    </Method>
